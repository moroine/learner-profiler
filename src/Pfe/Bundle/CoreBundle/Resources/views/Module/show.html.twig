{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Module</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ entity.type }}</td>
            </tr>
            <tr>
                <th>Category</th>
                <td>{{ entity.category }}</td>
            </tr>
            <tr>
                <th>Mooc_id</th>
                <td>{{ entity.moocid }}</td>
            </tr>
            <tr>
                <th>Section_mooc_id</th>
                <td>{{ entity.sectionmoocid }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('module') }}">
            Back to the list
        </a>
    </li>
</ul>
{% endblock %}
